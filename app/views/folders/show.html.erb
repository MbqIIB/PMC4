<h1>Folder  <i class="titleName"> <%= @folder.mpath %> <%= @folder.lfolder %>  </i ></h1>
<p id="notice"><%= notice %></p>

<p>
  <strong>Storage:</strong>
  <%= @folder.storage.name %>
</p>

<p>
  <strong>Mfile:</strong>
  <%= @folder.mfile_id %>
</p>

<%= link_to 'Edit', edit_folder_path(@folder) %> |
<%= link_to 'Back', folders_path %>

<%= render 'shared/mfiles' %>



<% @mfiles.each do |mfile| %>
      <div class="thumbD" id="<%=mfile.id%>" data-<%=mfile.id%>="a" 
     	class=" <% mfile.attris.each do |attri| %> <%=attri.name%> <% end %> 
             <% mfile.agroups.each do |agroup| %> <%=agroup.name%> <% end %> " >


<%= mfile.folder_id %>
        <%= mfile.filename %>

    <% end %>


