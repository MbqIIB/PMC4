<h1>Listing mfiles</h1>
    <% @mtypes.each do |mtype| %>
<p>
      <%= mtype[0].to_s + " "+mtype[1].to_s%>

    <%end %>
<table>
  <thead>
    <tr>
      <th>Folder/URL</th>
      <th>Name</th>
      <th>Modified</th>
      <th>Mod date</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @mfiles.each do |mfile| %>
      <tr>
      	<td><%= mfile.furlder %></td>
        <td><%= mfile.name %></td>
        <td><%= mfile.modified %></td>
        <td><%= mfile.mod_date %></td>
        <td><%= link_to 'Show', mfile %></td>
        <td><%= link_to 'Edit', edit_mfile_path(mfile) %></td>
        <td><%= link_to 'Destroy', mfile, method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mfile', new_mfile_path %>
